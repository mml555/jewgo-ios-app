# Environment Configuration for JewgoAppFinal Mobile App
# Copy this file to .env and update with your actual values

# API Configuration
# For production: https://jewgo-app-oyoh.onrender.com/api/v5
# For local development: http://localhost:3001/api/v5
API_BASE_URL=https://jewgo-app-oyoh.onrender.com/api/v5

# Environment (development, staging, production)
NODE_ENV=development

# Debug Mode (true/false)
DEBUG_MODE=true

# Google Services
GOOGLE_PLACES_API_KEY=your_google_places_api_key_here
GOOGLE_OAUTH_CLIENT_ID=your_google_oauth_client_id_here

# Analytics
ENABLE_ANALYTICS=false
ENABLE_PERFORMANCE_MONITORING=false

